    "\n",
    "def image_p(url: str) -> bool:\n",
    "    if mimetypes.guess_type(url)[0] is not None:\n",
    "        if \"image\" in mimetypes.guess_type(url)[0]:  # type: ignore\n",
    "            return True\n",
    "    return False\n"
